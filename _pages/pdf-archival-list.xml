---
layout: null
permalink: /pdf-archival-list.xml
sitemap: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% assign pdfs = site.static_files | where_exp: "f","f.extname == '.pdf'" | sort: "path" %}
{% for f in pdfs %}
	{% capture rel %}{{ f.path | replace: site.source, '' }}{% endcapture %}
	{% unless rel %}{% assign rel = f.name %}{% endunless %}
	{% unless rel contains '/' %}{% assign rel = '/' | append: rel %}{% endunless %}
	<url>
		<loc>{{ site.url | append: site.baseurl | append: rel | xml_escape }}</loc>
		<lastmod>{{ f.modified_time | date_to_xmlschema }}</lastmod>
	</url>
{% endfor %}
</urlset>
